{
  "name": "ochre-sdk",
  "version": "0.19.7",
  "type": "module",
  "license": "MIT",
  "description": "Node.js library for working with OCHRE (Online Cultural and Historical Research Environment) data",
  "author": "Firat Ciftci <firatciftci@uchicago.edu> (https://digitalculture.uchicago.edu)",
  "homepage": "https://github.com/uchicago-digitalculture-webdev/ochre-sdk",
  "bugs": {
    "url": "https://github.com/uchicago-digitalculture-webdev/ochre-sdk/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uchicago-digitalculture-webdev/ochre-sdk.git"
  },
  "keywords": [
    "ochre",
    "uchicago",
    "digital-culture",
    "digital-humanities",
    "typescript",
    "sdk",
    "parser",
    "cultural-heritage",
    "research"
  ],
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown src/index.ts --watch",
    "build": "tsdown",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache . --fix",
    "lint:nocache": "eslint .",
    "lint:nocache:fix": "eslint . --fix",
    "format": "prettier --cache --check .",
    "format:fix": "prettier --cache --write --list-different .",
    "check-types": "tsc --noEmit",
    "test": "vitest run",
    "release": "bumpp"
  },
  "dependencies": {
    "@date-fns/utc": "^2.1.1",
    "date-fns": "^4.1.0",
    "fast-equals": "^6.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@types/node": "^24.10.13",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.0",
    "prettier": "^3.8.1",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "packageManager": "pnpm@10.29.3"
}
